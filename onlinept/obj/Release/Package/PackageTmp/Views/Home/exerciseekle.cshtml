@model onlinept.Models.Entity.exercise

@{
    ViewBag.Title = "Öğrernciye Özel Egzersiz Ekle";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.link = "exerciseekle";
    ViewBag.menuactive = "Ayarlar2";
}

@if (TempData["exerciseHata"] != null)
{

    <script>
        Swal.fire('Hata', 'İşlem gerçekleştirilemedi', 'error');


            /**/</script>
}

<div class="content-wrapper">
    <div class="card-header">
        <h3 class="card-title">Öğrernciye Özel Egzersiz Ekle</h3>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        @using (Html.BeginForm("exerciseekle", "Home", FormMethod.Post, new { @enctype = "multipart/from-data" }))
        {
            <div class="form-group">
                @Html.TextBoxFor(m => m.mylistget, new { @class = "form-control", @Value = "", id = "mylist2" })
                <p>Öğrenci Seç</p>
                @Html.TextBoxFor(m => m.usersget, new { id = "colordbs", @Value = "", @class = "form-control", @required = "required" })
                <select id="colordb" class="form-control">
                    <option value=""></option>
                    @foreach (var user in ViewBag.UserList)
                    {
                        <option value="@user.userid">@user.name</option>
                    }
                </select>
            </div>
            <div class="form-group">
                <p>Tarih</p>
                @Html.EditorFor(m => m.date, new { htmlAttributes = new { @style = "width:600px", @class = "form-control", id = "tarih1", placeholder = "Lütfen Başlangıç Tarihi Giriniz.", @required = "required" } })
            </div>
            <div class="form-group">
                <p>Başlık </p>
                @Html.TextBoxFor(m => m.subject, new { @class = "form-control", oninput = "customtext(this.id);", @required = "required" })
            </div>
            <div class="form-group">
                <p>İçerik</p>
                @Html.TextAreaFor(m => m.descriptions, new { @class = "form-control", oninput = "customtext(this.id);", @required = "required" })
                @Html.TextBoxFor(m => m.exerciseid, new { oninput = "Number(this.id);", maxlength = "9", @id = "viewkey1", @class = "form-control", @Value = "", @readonly = "false" })
            </div>

            <div class="form-group">
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title"><i class="fas fa-dumbbell" aria-hidden="true"></i>  Egzersiz Ekle!</h3>
                        <div class="card-tools">
                            <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
                        </div>
                    </div>
                    <div class="card-body">

