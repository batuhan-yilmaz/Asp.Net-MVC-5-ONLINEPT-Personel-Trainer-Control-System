<script>
    $("#mobility_strechdb").add("#abs_coredb").add("#backdb").add("#biceps_forearmsdb").add("#chestdb").add("#leg_glutdb").add("#neckdb").add("#shoulderdb").add("#tricepsdb").change(function () {
        if ($('select#mobility_strechdb option:selected', this) || $('select#abs_coredb option:selected', this) || $('select#backdb option:selected', this) || $('select#biceps_forearmsdb option:selected', this) || $('select#chestdb option:selected', this) || $('select#leg_glutdb option:selected', this) || $('select#neckdb option:selected', this) || $('select#shoulderdb option:selected', this) || $('select#tricepsdb option:selected', this)) {

            if ($('#mobility_strechdb').is(':visible') || $('#abs_coredb').is(':visible') || $('#backdb').is(':visible') || $('#biceps_forearmsdb').is(':visible') || $('#chestdb').is(':visible') || $('#leg_glutdb').is(':visible') || $('#neckdb').is(':visible') || $('#shoulderdb').is(':visible') || $('#abs_coredb').is(':visible') || $('#tricepsdb').is(':visible')) {
                hardcore(this.value, "/Content/Admin/data/exercise/gifs/", this.options[this.selectedIndex].text);
                console.log(this.value);

            }
        }

    });
    var counterx = 1;
    function hardcore(val, loc, name) {
        if ((val != null) && (val != undefined) && (val != 0) && (counterx < 101)) {
            var img = document.createElement('img'),
                a = document.createElement('a'),
                div = document.createElement('div'),
                refdiv = document.getElementById('exercises'),
                button = document.createElement('button'),
                p = document.createElement('p'),
                description = document.createElement('textarea'),
                div2 = document.createElement('div');
            if ((document.getElementById("exercisedbs" + counterx).value == undefined || document.getElementById("exercisedbs" + counterx).value == 0)) {
                console.log(("exercisedbs" + counterx));
                $("#exercisedbs" + counterx).val(val);
            }
            else {
                for (i = 1; i < 101; i++) {
                    if (document.getElementById("exercisedbs" + i).value == "") {
                        $("#exercisedbs" + i).val(val);
                        counterx = i;
                        break;
                    }
                }
            }
            div2.setAttribute("style", "float:left; height:100px;");
            div.setAttribute("style", "height:120px;");
            a.setAttribute("data-toggle", "lightbox");
            a.setAttribute("data-gallery", "gallery");
            div.id = ("exgif" + counterx);
            p.innerHTML = (name);
            p.setAttribute("style", "color:#FF0000;");
            description.rows = "2";
            description.cols = "60";
            description.required = "required";
            a.href = (loc + val);
            img.src = (loc + val);
            img.setAttribute("style", "height:100px; float:left; width:100px;");
            description.type = "text";
            description.size = "60";
            description.id = (counterx);
            description.placeholder = "Eklediğiniz hareketle ilgili açıklama.. (Set,tekrar,yapılış biçimi vs..)";
            button.value = (counterx);
            button.setAttribute("style", "float:right");
            button.setAttribute("class", "fa fa-trash btn btn-danger");
            button.onclick = function () {
                $("#exercisedbs" + this.value).text(0);
                $("#exercisedbs" + this.value).val(0);
                $('#exdescdbs' + this.value).val(0);
                var style = document.getElementById("exgif" + this.value);
                style.parentNode.removeChild(style);
                console.log("Counterx:" + counterx);
                counterx = this.value;
            };
            refdiv.appendChild(div);
            div.appendChild(a);
            a.appendChild(img);
            div.appendChild(div2);
            div2.appendChild(button);
            div2.appendChild(p);
            div2.appendChild(description);
            $('textarea').on('keyup', function () {
                var x = document.getElementById("exdescdbs" + this.id);
                $(x).val(name + ":" + $(this).val());
            });
            counterx++;
        }
        else {
            alert("Daha fazla egzersiz ekleyemezsiniz.");
        }

    }

</script>
